<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
   <typeAliases>
      <typeAlias type="model.StoreDTO" alias="storedto" />
      <typeAlias type="model.StoreoptDTO" alias="storeoptdto" />
      <typeAlias type="model.GpaylistDTO" alias="gpaylistdto" />
       <typeAlias type="model.PayDTO" alias="paydto" />
       <typeAlias type="model.Member" alias="Member" />
      <typeAlias type="model.BookList" alias="BookList" />
      <typeAlias type="model.Movie" alias="Movie" />
      <typeAlias type="model.Theater" alias="Theater" />
      <typeAlias type="model.TimeTable" alias="TimeTable" />
      <typeAlias type="model.Screen" alias="Screen" />
      <typeAlias type="model.TimeTableScreen" alias="TimeTableScreen" />
        <typeAlias type="model.CustomerDTO" alias="customerdto" />
      <typeAlias type="model.GradeDTO" alias="gradedto" />
       <typeAlias type="model.EventDTO" alias="eventdto" />
      
      
   </typeAliases>
   
   <environments default="development">
      <environment id="development">
         <transactionManager type="JDBC" />
         <dataSource type="POOLED">
            <property name="driver" value="oracle.jdbc.driver.OracleDriver" />
            <property name="url" value="jdbc:oracle:thin:@localhost:1521:xe" />
            <property name="username" value="SMRIT" />
            <property name="password" value="oracle" />
         </dataSource>
      </environment>
   </environments>
   <mappers>
      <mapper resource="repository/mapper/StoreMapper.xml" />      
      <mapper resource="repository/mapper/MemberMapper.xml" />
     <mapper resource="repository/mapper/MoviebookMapper.xml" />
       <mapper resource="repository/mapper/MovieAdminMapper.xml" />
 	<mapper resource="repository/mapper/MovieUserMapper.xml" />
 	  <mapper resource="repository/mapper/CustomerMapper.xml"/>
      <mapper resource="repository/mapper/GradeMapper.xml"/>
<mapper resource="repository/mapper/EventMapper.xml" />
   </mappers>
</configuration>